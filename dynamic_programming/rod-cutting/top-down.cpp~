#include<iostream>
#include<cstdio>
#include<algorithm>

using namespace std;

int price[] = {0,1,5,8,9};
int cost[5] = {0};

int maxProfit(int n) {
	if(n==1) return price[1];
	
	for(int i=1;i<n;i++) {
		cost[n] = max(price[n],maxProfit(n-i)+price[i]);
		
	}
	return cost[n];
}


int main() {
	cost[1] = price[1];
	cout<<maxProfit(4)<<endl;
	for(int i=0;i<5;i++) cout<<cost[i]<<" ";
	return 0;	
}
